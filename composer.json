{
  "name": "mindbird/contao-be-formbox",
  "description": "Die Erweiterung bietet Backendbenutzern die Möglichkeit über ein Formular Kontakt mit z.B. dem Support aufzunehmen.",
  "minimum-stability": "stable",
  "license": "proprietary",
  "keywords": [
    "contao",
    "Kontakt",
    "Support",
    "Anregungen"
  ],
  "type": "contao-module",
  "authors": [
    {
      "name": "mindbird",
      "email": "info@mindbird.de"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "contao/core": ">=3.2,<4"
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/be-formbox"
      }
    }
  },
  "autoload": {
    "psr-4": {
      "BeFormbox\\": "src/"
    }
  },
  "replace": {
    "contao-legacy/be-formbox": "self.version"
  }
}